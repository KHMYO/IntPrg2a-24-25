@model List<Person>
<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Lastname</th>
            <th>Birth Place</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var kisi in Model)
        {
            <tr>
                <td>@kisi.Id</td>
                <td>@kisi.Name</td>
                <td>@kisi.Lastname</td>
                <td>@kisi.BirthPlace</td>
            </tr>        
        }
    </tbody>
</table>
